  <kop>
    <label>Artikel</label>
    <nr>8</nr>
  </kop>
  <al>Deze wet kan worden aangehaald als: Wet vrijwillige zetelverplaatsing derde landen.</al>
<meta-data><brondata><oorspronkelijk><publicatie effect="nieuwe-regeling" soort="Stb" urlidentifier=""><publicatiejaar>1994</publicatiejaar><publicatienr>800</publicatienr><ondertekeningsdatum isodatum="1994-10-13">13-10-1994</ondertekeningsdatum><dossierref dossier="23316">23316</dossierref></publicatie></oorspronkelijk><inwerkingtreding><publicatie effect="nieuwe-regeling" soort="Stb" urlidentifier=""><publicatiejaar>1994</publicatiejaar><publicatienr>800</publicatienr><ondertekeningsdatum isodatum="1994-10-13">13-10-1994</ondertekeningsdatum><dossierref dossier="23316">23316</dossierref></publicatie><inwerkingtreding.datum isodatum="1994-11-23">23-11-1994</inwerkingtreding.datum></inwerkingtreding></brondata><jcis><jci versie="1.0" verwijzing="1.0:c:BWBR0006968&amp;artikel=8&amp;g=2011-07-01" onderdeel="artikel=8" korte-verwijzing="1.0:c:BWBR0006968&amp;artikel=8"/><jci versie="1.3" verwijzing="jci1.3:c:BWBR0006968&amp;artikel=8&amp;z=2011-07-01&amp;g=2011-07-01" onderdeel="artikel=8" korte-verwijzing="jci1.3:c:BWBR0006968&amp;artikel=8"/></jcis></meta-data></artikel>
