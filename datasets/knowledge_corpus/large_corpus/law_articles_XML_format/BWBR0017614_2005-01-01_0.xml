      <artikel bwb-ng-variabel-deel="/ArtikelIII" stam-id="6560503" versie-id="11363612" id="C15336371" label-id="6548844" inwerking="2005-01-01" label="Artikel III" bron="Stb.2004-705" effect="nieuwe-regeling" ondertekening_bron="2004-12-02" publicatie_bron="2004-12-29" publicatie_iwt="2004-12-29" status="goed">
