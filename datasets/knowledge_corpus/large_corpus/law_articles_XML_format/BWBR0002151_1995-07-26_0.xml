  <lid bwb-ng-variabel-deel="/Artikel1/Lid1" label-id="1355174L1">
    <lidnr>1</lidnr>
    <al>Er is een Productschap voor Gedistilleerde Dranken.</al>
  <meta-data><jcis><jci versie="1.3" verwijzing="jci1.3:c:BWBR0002151&amp;artikel=1&amp;lid=1&amp;z=1995-07-26&amp;g=1995-07-26" onderdeel="lid=1"/></jcis></meta-data></lid>
