      <artikel bwb-ng-variabel-deel="/Artikel2" code="a-2" stam-id="1397613" versie-id="2096262" id="C5217341" label-id="1355184" inwerking="1956-01-01" label="Artikel 2" bron="Stb.1954-450" effect="nieuwe-regeling" ondertekening_bron="1954-09-30" status="goed">
